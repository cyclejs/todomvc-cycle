{
  "name": "todomvc-cycle",
  "version": "0.0.0",
  "author": "Andre Staltz",
  "repository": {
    "type": "git",
    "url": "git@github.com:staltz/todomvc-cycle.git"
  },
  "license": "MIT",
  "private": true,
  "contributors": [
    {
      "name": "Frederik Krautwald"
    },
    {
      "name": "Kahlil Lechelt",
      "email": "hello@kahlil.info"
    }
  ],
  "dependencies": {
    "@cycle/dom": "17.4",
    "@cycle/history": "6.2",
    "@cycle/isolate": "3.0",
    "@cycle/run": "3.1",
    "@cycle/storage": "4.0",
    "cycle-onionify": "4.0.0-rc.10",
    "cycle-storageify": "3.0",
    "todomvc-app-css": "2.1.x",
    "todomvc-common": "1.0.3",
    "xstream": "10.8"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "12.0.1",
    "live-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.4.0",
    "patch-package": "^2.0.0",
    "tape": "^4.8.0",
    "uglify-js": "2.6.1",
    "watchify": "^3.6.1",
    "yarn": "^0.24.4"
  },
  "scripts": {
    "prepare": "patch-package",
    "build-debug": "mkdirp js && browserify src/app.js -t babelify --outfile js/app.js",
    "watch:js": "mkdirp js && watchify src/app.js -t babelify --outfile js/app.js -dv",
    "serve": "live-server ./",
    "uglify": "uglifyjs js/app.js -o js/app.min.js",
    "build": "npm run build-debug && npm run uglify",
    "start": "npm-run-all --parallel watch:js serve",
    "test": "tape -r babel-register src/**/*.test.js"
  }
}
